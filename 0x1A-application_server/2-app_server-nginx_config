# NGINIX server configuration
# Serving locally and on its public IP on port 80
# proxies requests to the process listening on port 5000.
server {
    listen 80;
    web-01 34.229.67.178;

    location /airbnb-onepage/ {
        proxy_pass http://127.0.0.1:5000;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
    }
}
